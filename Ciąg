import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		ciag();
	}
	public static void ciag(){
		String n;
		int nl;
		
		n = JOptionPane.showInputDialog("Podaj ilość pierwszych wyrazów ciągu fibonacciego, które mam Ci podać");
		nl = Integer.parseInt(n);
		
		int[] a = new int[nl];
		a[0] = 1;
		a[1] = 1;
		
		JOptionPane.showMessageDialog(null, "a0 = 1", "Wynik", JOptionPane.INFORMATION_MESSAGE);
		JOptionPane.showMessageDialog(null, "a1= 1 ", "Wynik", JOptionPane.INFORMATION_MESSAGE);
		for(int i = 2; i <= nl; i++){
			int b = i - 2;
			int c = i - 1;
			
			a[i] = a[b] + a[c];
			
			JOptionPane.showMessageDialog(null, b + " wyraz ciągu fibonacciego i " + c + " wyraz ciągu fibonacciego daje nam wynik równy a" + i + " = " + a[i], "Wynik", JOptionPane.INFORMATION_MESSAGE);
		}
	}
}
