package me.seba1549.main;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		String a, b;
		int al, bl;
		
		a = JOptionPane.showInputDialog("Podaj pierwszą liczbę: ");
		al = Integer.parseInt(a);
		b = JOptionPane.showInputDialog("Podaj drugą liczbę: ");
		bl = Integer.parseInt(b);
		
		while(al != bl){
			if(al > bl){
				al -= bl;
			}
			else{
				bl -= al;
			}
		}
		
		JOptionPane.showMessageDialog(null, "NWD podanych przez Ciebie liczb to: " + al);
		
	}

}
