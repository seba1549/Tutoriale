package me.seba1549.main;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		String n, pomocna = "";
		int nl, max = 0;
		
		
		while(!(pomocna.equals("0"))){
			n = JOptionPane.showInputDialog("Podaj n: ");
			pomocna = n;
			nl = Integer.parseInt(n);
			if(max < nl){
				max = nl;
			}
		}
		
		JOptionPane.showMessageDialog(null, "Max jest rÃ³wne: " + max, "KOMUNIKAT", JOptionPane.INFORMATION_MESSAGE);

	}

}
